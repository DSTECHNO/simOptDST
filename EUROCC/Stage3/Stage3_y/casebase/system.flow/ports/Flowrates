    Q_Outlet  // Name also used to identify output folder
    {
        type            faceSource;
        functionObjectLibs ("libfieldFunctionObjects.so");
        enabled         true;
        outputControl   timeStep;
        outputInterval  1;
        log             true;
        valueOutput     false;
        source          patch;  // Type of face source: faceZone, patch
        sourceName      outlet;
        operation       sum;
        fields
        (
            phi
        );
    }

    Q_Inlet  // Name also used to identify output folder
